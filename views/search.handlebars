<div class="search-header level">
    <h2 class="is-size-1 search-title ">Search</h2>
    <p class="is-size-5 ">Search for a launch by launch name or rocket name</p>
    <form class="searchBox">
        <input type="text" name="name" class="search-bar is-size-3" required>
        <input type="submit" value="Search" id="postSearch" class="button is-success"></input>
    </form>
</div>
</div>
{{#if noResults}} 
<div class="is-size-3 noResults">No Launches Found</div>
{{/if}}
